%html{lang: "es"}
  /
    <![endif]
  %head
    %meta{charset: "utf-8"}/
    %title Reportes - AdminChurch
    %meta{content: "CLM Developers", name: "author"}/
    %meta{content: "width=device-width, initial-scale=1.0, user-scalable=no", name: "viewport"}/
    %meta{content: "Admin Church", name: "description"}/
    %meta{content: "Admin Chuch, CLM Developers, Administración Eclesiástica, Iglesia", name: "keywords"}/
    = javascript_include_tag "html5-trunk.js"
    = stylesheet_link_tag "icomoon/style.css"
    /[if lte IE 7]
      <script src="icomoon-font/lte-ie7.js"></script>
    / bootstrap css
    = stylesheet_link_tag "bootstrap.css"
    / custom css
    = stylesheet_link_tag "main"
    = stylesheet_link_tag "datepicker/css/datepicker.css"
    = stylesheet_link_tag "mapa/jquery-gmaps-lation-picker"
    = stylesheet_link_tag "smooth/jquery-ui-1.8.16.custom"
    = stylesheet_link_tag "loader"
    = stylesheet_link_tag "successblock"
    = stylesheet_link_tag "jqueryvalidation/css/validationEngine.jquery.css"
  %body
    / Header start
    %header
      / Logo start
      .logo
        = image_tag('logo-ac-103.png', alt: "Admin Church", style: "border-bottom: 1px solid #383838;") 
      / Logo end
      / Optional Dropdown start
      #optional-dropdown
        %ul
          %li
            %a{href: "index.html"}
              %span.fs1{"aria-hidden" => "true", "data-icon" => ""}
      / Optional Dropdown end
      / Mini navigation start
      #mini-nav
        %ul
          %li.hidden-sm
            %a{href: "mail.html"}
              %span.fs1{"aria-hidden" => "true", "data-icon" => ""}
          %li
            %a{href: "login.html"}
              %span.text-label Mr. John
              %span.fs1{"aria-hidden" => "true", "data-icon" => ""}
            %ul.user-summary
              %li
                .summary
                  .user-pic
                    %img{alt: "Slick Admin", src: "/images/avatar-1.png"}/
                  .basic-details
                    %h4.no-margin Johnny
                    %h5.no-margin UX Designer
                    %small Garabandha, India
                  .profile-progress
                    .chart-progress{"data-percent" => "68"}
                      68%
                  .clearfix
              %li
                %button.btn.btn-xs.btn-danger.pull-right{onclick: "location.href='login.html'"} Logout
                %span.clearfix
      / Mini navigation end
    / Header end
    / Main Container start
    .main-container
      / Main Container start
    .main-container
      / Mian navigation start
      #mainnav
        %ul
          %li
            %a{href: persona_path}
              .icon
                %span.fs1{"aria-hidden" => "true", "data-icon" => ""}
              Persona
          %li
            %a{href: diezmos_path}
              .icon
                %span.fs1{"aria-hidden" => "true", "data-icon" => ""}
              Diezmos
          %li
            %a{href: ofrendas_path}
              .icon
                %span.fs1{"aria-hidden" => "true", "data-icon" => ""}
              Ofrendas
          %li
            %a{href: asistencia_path}
              .icon
                %span.fs1{"aria-hidden" => "true", "data-icon" => ""}
              Asistencia
          %li
            %a{href: "#"}
              .icon
                %span.fs1{"aria-hidden" => "true", "data-icon" => ""}
              Información
            %ul
              %li
                %a{href: dashboard_asistencia_path} Asistencia
              %li
                %a{href: dashboard_diezmo_path} Diezmos
              %li
                %a{href: dashboard_ofrenda_path} Ofrendas
              %li
                %a{href: dashboard_membresia_path} Miembros/Visitantes
          %li
            %a{href: "#"}
              .icon
                %span.fs1{"aria-hidden" => "true", "data-icon" => ""}
              Reportes
            %ul
              %li
                %a{href: reporte_asistencia_path} Asistencia
              %li
                %a{href: reporte_diezmo_path} Diezmos
              %li
                %a{href: reporte_ofrenda_path} Ofrendas
              %li
                %a{href: reporte_membresia_path} Miembros/Visitantes
              %li
                %a{href: reporte_servicios_path} Servicios
          %li
            %span.current-arrow
            %a{href: "#"}
              .icon
                %span.fs1{"aria-hidden" => "true", "data-icon" => ""}
              Configuración
            %ul
              %li
                %a{href: configuracion_datos_generales_path} Datos Generales
              %li
                %a{href: configuracion_lugar_path} Lugar
              %li
                %a{href: configuracion_usuario_path} Usuarios
              %li
                %a{href: configuracion_servicios_path} Servicios
      / Mian navigation end
      = yield
      / Dashboard wrapper end
    / Main Container end

    / Scripts
    :javascript
      urlES = "/dt_ES.txt"
    = javascript_include_tag :application
    = javascript_include_tag "https://maps.googleapis.com/maps/api/js?sensor=true"
    = javascript_include_tag :configuracion
    
    =yield :bloquejavascript