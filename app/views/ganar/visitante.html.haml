/ Dashboard wrapper start
.dashboard-wrapper
  / Page title start
  .row.page-title
    %h2
      Visitante
      %small
        %li Inicio
        %li /
        %li Persona
        %li /
        %li Visitante
  / Page title end
  / Row start
  .row
  .col-md-12
    .widget
      .widget-header
        .title
          %span.fs1{"aria-hidden" => "true", "data-icon" => ""}
          Registrar Visitante
      .widget-body
        %form.form-horizontal{role: "form"}
          #wizard
            %ol
              %li Datos Personales
              %li Directorio
              %li Datos Finales
            %div
              .form-group
                %label.col-lg-2.control-label{for: "nombre"} Nombre
                .col-lg-10
                  .row
                    .col-lg-2.col-sm-2
                      %input#nombre.form-control{name: "nombre", type: "text"}
                    %label.col-lg-2.col-sm-2.control-label Apellido
                    .col-lg-2.col-sm-2
                      %input#apellido.form-control{name: "apellido", type: "text"}
                    %label.col-lg-2.col-sm-2.control-label Edad
                    .col-lg-2.col-sm-2
                      %input#edad.form-control{name: "edad", type: "text"}
            %div
              .form-group
                %label.col-lg-2.control-label{for: "telefono"} Teléfono
                .col-lg-10
                  .row
                    .col-lg-2.col-sm-2
                      %input#telefono.form-control{name: "telefono", type: "text"}
                    %label.col-lg-2.col-sm-2.control-label Tipo
                    .col-lg-2.col-sm-2
                      %select#tipo_tel.form-control{name: "tipo_tel"}
                    %label.col-lg-2.col-sm-2.control-label 
                    .col-lg-2.col-sm-2
                      %button.btn.btn-default{"data-original-title" => "", type: "button"} +
              #dt_example.example_alt_pagination.col-lg-8.col-lg-offset-2
                %table#data-table.table.table-condensed.table-striped.table-hover.table-bordered.pull-left
                  %thead
                    %tr
                      %th{style: "width:33%"} Teléfono
                      %th.hidden-phone{style: "width:33%"} Tipo
                      %th.hidden-phone{style: "width:33%"} Acciones
                  %tbody
                    %tr.gradeX
                      %td John
                      %td.hidden-phone $22590
                      %td.hidden-phone
                        %a.delete-row{"data-original-title" => "Eliminar", href: "#"}
                          %img{alt: "trash", src: "http://d9i0z8gxqnxp1.cloudfront.net/img/trash-icon.png"}/
                    %tr.gradeA
                      %td Ruchita
                      %td.hidden-phone $98450
                      %td.hidden-phone
                        %a.delete-row{"data-original-title" => "Eliminar", href: "#"}
                          %img{alt: "trash", src: "http://d9i0z8gxqnxp1.cloudfront.net/img/trash-icon.png"}/
                    %tr.gradeU
                      %td Barote babu
                      %td.hidden-phone $99876
                      %td.hidden-phone
                        %a.delete-row{"data-original-title" => "Eliminar", href: "#"}
                          %img{alt: "trash", src: "http://d9i0z8gxqnxp1.cloudfront.net/img/trash-icon.png"}/
              .clearfix
            %div
              .form-group
                %label.col-lg-2.control-label{for: "monto"} Invitado por
                .col-lg-5
                  %input#invitado.form-control{name: "invitado", type: "text"}
              .form-group
                %label.col-lg-2.control-label{for: "mot_oracion"} Motivo de Oración
                .col-lg-5
                  %input#mot_oracion.form-control{name: "mot_oracion", type: "text"}
              .form-group
                .col-lg-offset-2.col-lg-10
                  %button.btn.btn-default{type: "button"} Cancelar
                  %button.btn.btn-success{type: "submit"} Guardar
  / Row end
