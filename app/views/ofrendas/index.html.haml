/ Dashboard wrapper start
.dashboard-wrapper
  / Page title start
  .row.page-title
    %h2
      Ofrendas
      %small
        %li Home
        %li /
        %li Ofrendas
  / Page title end
  / Row start
  .row
    .col-md-12
      .widget
        .widget-header
          %a.btn#registrar_ofrenda.btn-primary.btn-block{ href: "#collapseOne"}
            Agregar Ofrenda
        %div#ofrenda_div
          .widget-body
            = form_tag(ofrendas_guardar_path , {:method=>'post',:class=>'form-horizontal' , :id => "form_ofrenda" , :name=> "form_ofrenda"}) do
              .form-group
                %label.col-lg-2.control-label{for: "servicio"} Servicio
                .col-lg-10                  
                  = servicios_select()

              .form-group
                %label.col-lg-2.control-label{for: "turno"} Turno
                .col-lg-10                  
                  %select#turno{name: "turno", class: "form-control"}

              .form-group
                %label.col-lg-2.control-label{for: "fecha"} Fecha
                .col-lg-10
                  %input#fecha.form-control{name: "fecha", type: "date"}/
              .form-group
                %label.col-lg-2.control-label{for: "monto"} Monto
                .col-lg-10
                  %input#monto.form-control{name: "monto", type: "text"}/
              .form-group
                .col-lg-offset-2.col-lg-10
                  %button.btn.btn-default{type: "button"} Cancelar
                  %button#btnGuardar_Ofrenda.btn.btn-success{type: "button"} Guardar
  / Row end
