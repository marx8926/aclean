/ Dashboard wrapper start
.dashboard-wrapper
  / Page title start
  .row.page-title
    %h2
      Lugar
      %small
        %li Home
        %li /
        %li Lugar
  / Page title end
  / Row start
  .row
    .col-md-12
      .widget
        .widget-body
          .row
            .col-md-6.col-sm-6
              .widget
                .widget-header
                  .title 
                    %span.fs1{"aria-hidden" => "true", "data-icon" => ""}
                    Dashboard
                %form#form_diezmo.widget-body{name: "form_diezmo", method: "post"}
                  .form-group
                    %label.col-lg-2.control-label{for: "anio"} Año
                    .col-lg-10
                    .row
                      .col-lg-2.col-sm-2
                        %input#anio.form-control{name: "anio", type: "text"}
                      %label.col-lg-2.col-sm-2.control-label Mes
                      .col-lg-2.col-sm-2
                        %input#mes{type: "checkbox", name: "mes"}/
                        Mes
                        %select#mes_lista.form-control{name: "mes_lista"}
                          %option{value: "1"} Enero
                          %option{value: "2"} Febrero
                          %option{value: "3"} Marzo
                          %option{value: "4"} Abril
                          %option{value: "5"} Mayo
                          %option{value: "6"} Junio
                          %option{value: "7"} Julio
                          %option{value: "8"} Agosto
                          %option{value: "9"} Setiembre
                          %option{value: "10"} Octubre
                          %option{value: "11"} Noviembre
                          %option{value: "12"} Diciembre
                      .col-lg-2.col-sm-2
                        %input#semana{type: "checkbox", name: "semana"}/
                        Semana
                        %select#semana_lista.form-control{name: "semana_lista"}
                          %option{value: "1"} 1°
                          %option{value: "2"} 2°
                          %option{value: "3"} 3°
                          %option{value: "4"} 4°

                    
                  .form-group
                    .col-lg-offset-2.col-lg-2
                      %input#btnGenerar.btn.btn-success{type: "button", name: "btnGenerar"} Generar
                          
            
          .row
            .col-md-6.col-sm-6
              .widget
                .widget-header
                  .title 
                    %span.fs1{"aria-hidden" => "true", "data-icon" => ""}
                    Dashboard
                .widget-body
                  #dash_pie

            
