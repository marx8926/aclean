/ Dashboard wrapper start
.dashboard-wrapper
  / Page title start
  .row.page-title
    %h2
      Ofrenda
      %small
        %li Home
        %li /
        %li Dashboard
        %li /
        %li Ofrenda
  / Page title end
  / Row start
  .row
    .col-md-12
      .widget
        .row
          .col-md-12.col-sm-12
            .widget
              .widget-header
                .title 
                  %span.fs1{"aria-hidden" => "true", "data-icon" => ""}
                    Estadísticas
              .widget-body
                %form#form_diezmo.form-horizontal{name: "form_diezmo", method: "post"}
                  .form-group
                    %label.col-lg-2.control-label{for: "servicio"} Servicio
                    .col-lg-10
                      .row
                        .col-lg-2.col-sm-2
                          %select#servicio.form-control{name: "servicio"}
                        %label.col-lg-2.col-sm-2.checkbox-inline{style:"text-align:right;"}
                          %input#semana{type: "checkbox", name: "semana", style:"float:none;"}
                          Semana
                        .col-lg-2.col-sm-2
                          %select#semana_lista.form-control{name: "semana_lista"}
                            %option{value: "1"} 1°
                            %option{value: "2"} 2°
                            %option{value: "3"} 3°
                            %option{value: "4"} 4°
                        .col-lg-2.col-sm-2
                          %button#btnGenerar_diezmo.btn.btn-success{type: "button", name: "btnGenerar_diezmo"} 
                            Generar
          .row
            .col-md-12.col-sm-12
              .widget
                .widget-body
                  #dash_pie